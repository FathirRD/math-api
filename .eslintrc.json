{
    "env": {
        "node": true,
        "commonjs": true,
        "jest": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": [
        "error", {
          "allowAfterThis": true,
          "allow": ["_exports"]
        }]
    }
}
